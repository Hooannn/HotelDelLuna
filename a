[32m+[m[32mpackage com.ht.hoteldelluna.models;[m
[32m+[m
[32m+[m[32mimport io.github.palexdev.materialfx.utils.RandomUtils;[m
[32m+[m
[32m+[m[32mimport java.util.Objects;[m
[32m+[m
[32m+[m[32mpublic class Person {[m
[32m+[m[32m    private final String name;[m
[32m+[m[32m    private final String surname;[m
[32m+[m[32m    private int age;[m
[32m+[m
[32m+[m[32m    public Person(String name) {[m
[32m+[m[32m        this.name = name;[m
[32m+[m[32m        this.surname = "";[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public Person(String name, String surname) {[m
[32m+[m[32m        this.name = name;[m
[32m+[m[32m        this.surname = surname;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public Person(String name, String surname, int age) {[m
[32m+[m[32m        this.name = name;[m
[32m+[m[32m        this.surname = surname;[m
[32m+[m[32m        this.age = age;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static Person ofSplit(String fullName, String split) {[m
[32m+[m[32m        String[] fNameArray = fullName.split(split);[m
[32m+[m[32m        return new Person(fNameArray[0], fNameArray[1]);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public String getName() {[m
[32m+[m[32m        return name;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public String getSurname() {[m
[32m+[m[32m        return surname;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public int getAge() {[m
[32m+[m[32m        return age;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void setAge(int age) {[m
[32m+[m[32m        this.age = age;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public Person randomAge() {[m
[32m+[m[32m        setAge(RandomUtils.random.nextInt(18, 81));[m
[32m+[m[32m        return this;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public boolean equals(Object o) {[m
[32m+[m[32m        if (this == o)[m
[32m+[m[32m            return true;[m
[32m+[m[32m        if (o == null || getClass() != o.getClass())[m
[32m+[m[32m            return false;[m
[32m+[m[32m        Person person = (Person) o;[m
[32m+[m[32m        return getName().equals(person.getName());[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public int hashCode() {[m
[32m+[m[32m        return Objects.hash(getName());[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
