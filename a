[32m+[m[32m        roomNameTextField.setText(room.getName());[m
[32m+[m[32m        roomTypeTextField.setText(room.getType().getName());[m
[32m+[m
[32m+[m[32m        if (room.getStatus() == RoomStatus.AVAILABLE) {[m
[32m+[m[32m            checkOutDateTimeTextField.setDisable(true);[m
[32m+[m[32m        } else if (room.getStatus() == RoomStatus.OCCUPIED && reservation != null) {[m
[32m+[m[32m            checkInDateTimeTextField.setDisable(true);[m
[32m+[m[32m            totalCustomersTextField.setDisable(true);[m
[32m+[m[32m            customerNameTextField.setDisable(true);[m
[32m+[m[32m            checkInDateTimeTextField.setLocalDateTime(LocalDateTime.parse(reservation.getCheckInTime()));[m
[32m+[m[32m            totalCustomersTextField.setText(String.valueOf(reservation.getCustomerCount()));[m
[32m+[m[32m            customerNameTextField.setText(reservation.getCustomerName());[m
[32m+[m[32m            noteTextArea.setText(reservation.getNote());[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
