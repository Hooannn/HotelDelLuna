 public class RoomServices {[m
[32m+[m[32m    private final Parser parser = new Parser();[m
     Connection dbConnection = Connection.shared;[m
[32m+[m[32m    MongoCollection<Document> roomCollection = dbConnection.getDatabase().getCollection("rooms");[m
[32m+[m[32m    MongoCollection<Document> roomTypeCollection = dbConnection.getDatabase().getCollection("room_types");[m
[32m+[m[32m    MongoCollection<Document> floorCollection = dbConnection.getDatabase().getCollection("floors");[m
[32m+[m
[32m+[m[32m    public List<Room> getRooms() {[m
[32m+[m[32m        List<Document> documents = new ArrayList<>();[m
[32m+[m[32m        roomCollection.find().into(documents);[m
[32m+[m[32m        documents.forEach(this::populateRoomDocument);[m
[32m+[m[32m        return parser.fromDocuments(documents, Room.class);[m
[32m+[m[32m    }[m
[32m+[m[32m    public Room getRoomById(String roomId) {[m
[32m+[m[32m        Document doc = roomCollection.find(new Document("_id", new ObjectId(roomId))).first();[m
[32m+[m[32m        assert doc != null;[m
[32m+[m[32m        populateRoomDocument(doc);[m
[32m+[m[32m        return parser.fromDocument(doc, Room.class);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public InsertOneResult addRoom(Room room, String roomTypeId, String floorId) {[m
[32m+[m[32m        Document document = parser.toDocument(room);[m
[32m+[m[32m        document.put("floor", new ObjectId(floorId));[m
[32m+[m[32m        document.put("type", new ObjectId(roomTypeId));[m
[32m+[m[32m        return roomCollection.insertOne(document);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public DeleteResult deleteRoom(String roomId) {[m
[32m+[m[32m        return roomCollection.deleteOne(new Document("_id", new ObjectId(roomId)));[m
[32m+[m[32m    }[m
[32m+[m
