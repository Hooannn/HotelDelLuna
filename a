[32m+[m[32m    Connection dbConnection = Connection.shared;[m[41m[m
[32m+[m[32m    MongoCollection<Document> invoiceCollection = dbConnection.getDatabase().getCollection("invoices");[m[41m[m
[32m+[m[32m    MongoCollection<Document> roomCollection = dbConnection.getDatabase().getCollection("rooms");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public List<Invoice> getInvoices() {[m[41m[m
[32m+[m[32m        List<Document> documents = new ArrayList<>();[m[41m[m
[32m+[m[32m        invoiceCollection.find().sort(new Document("checkOutTime", -1)).into(documents);[m[41m[m
[32m+[m[32m        documents.forEach(this::populateRoomDocument);[m[41m[m
[32m+[m[32m        return parser.fromDocuments(documents, Invoice.class);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public Invoice getInvoiceById(String invoiceId) {[m[41m[m
[32m+[m[32m        Document doc = invoiceCollection.find(new Document("_id", new ObjectId(invoiceId))).first();[m[41m[m
[32m+[m[32m        assert doc != null;[m[41m[m
[32m+[m[32m        populateRoomDocument(doc);[m[41m[m
[32m+[m[32m        return parser.fromDocument(doc, Invoice.class);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public InsertOneResult addInvoice(Invoice invoice) {[m[41m[m
[32m+[m[32m        Document document = parser.toDocument(invoice);[m[41m[m
[32m+[m[32m        return invoiceCollection.insertOne(document);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public InsertManyResult addMockInvoices() {[m[41m[m
[32m+[m[32m        List<Invoice> invoices = new ArrayList<>();[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        try {[m[41m[m
[32m+[m[32m            SimpleDateFormat dateFormat = new SimpleDateFormat("yyyy-MM-dd");[m[41m[m
[32m+[m[41m[m
[32m+[m[32m            invoices.add(new Invoice(dateFormat.parse("2023-11-01"), new Date(), 400000, "Ha Gia Huy 1", null));[m[41m[m
[32m+[m[32m            invoices.add(new Invoice(dateFormat.parse("2023-11-05"), new Date(), 300000, "Ha Gia Huy 2", null));[m[41m[m
[32m+[m[32m            invoices.add(new Invoice(dateFormat.parse("2023-11-11"), new Date(), 200000, "Ha Gia Huy 3", null));[m[41m[m
[32m+[m[32m        } catch (ParseException e) {[m[41m[m
[32m+[m[32m            e.printStackTrace();[m[41m[m
[32m+[m[32m        }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        List<Document> documents = parser.toDocuments(invoices);[m[41m[m
[32m+[m[32m        documents.forEach(document -> {[m[41m[m
[32m+[m[32m            document.put("room", new ObjectId("654df870eb51b55fc617a543"));[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[32m        return invoiceCollection.insertMany(documents);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    private void populateRoomDocument(Document document) {[m[41m[m
[32m+[m[32m        ObjectId roomId = document.getObjectId("room");[m[41m[m
[32m+[m[32m        Document room = roomCollection.find(new Document("_id", roomId)).first();[m[41m[m
[32m+[m[32m        document.put("room", room);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
