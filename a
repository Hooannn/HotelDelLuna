[32m+[m[32m        thirdDocuments.forEach(document -> {[m
[32m+[m[32m            document.put("floor", new ObjectId("654c38a7f9dc152c6575314f"));[m
[32m+[m[32m            document.put("type", new ObjectId("654df75ece6981358479f11f"));[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        List<Document> documents = new ArrayList<>();[m
[32m+[m[32m        documents.addAll(firstDocuments);[m
[32m+[m[32m        documents.addAll(secondDocuments);[m
[32m+[m[32m        documents.addAll(thirdDocuments);[m
[32m+[m
[32m+[m[32m        return roomCollection.insertMany(documents);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private void populateRoomDocument(Document document) {[m
[32m+[m[32m        ObjectId floorId = document.getObjectId("floor");[m
[32m+[m[32m        ObjectId roomTypeId = document.getObjectId("type");[m
[32m+[m[32m        Document floor = floorCollection.find(new Document("_id", floorId)).first();[m
[32m+[m[32m        Document roomType = roomTypeCollection.find(new Document("_id", roomTypeId)).first();[m
[32m+[m[32m        document.put("floor", floor);[m
[32m+[m[32m        document.put("type", roomType);[m
[32m+[m[32m    }[m
 }[m
