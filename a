[32m+[m[32m    public  boolean updateRoom(String roomId, String name, String roomTypeId, String floorId) {[m
[32m+[m[32m        String query = "UPDATE rooms SET name = ?, type = ?, floor = ? WHERE id = ?";[m
[32m+[m[32m        try (PreparedStatement preparedStatement = dbConnection.prepareStatement(query)) {[m
[32m+[m[32m            preparedStatement.setString(1, name);[m
[32m+[m[32m            preparedStatement.setString(2, roomTypeId);[m
[32m+[m[32m            preparedStatement.setString(3, floorId);[m
[32m+[m[32m            preparedStatement.setString(4, roomId);[m
[32m+[m[32m            int rowsAffected = preparedStatement.executeUpdate();[m
[32m+[m[32m            return rowsAffected > 0;[m
[32m+[m[32m        } catch (SQLException e) {[m
[32m+[m[32m            e.printStackTrace();[m
[32m+[m[32m            return false;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
