[32m+[m[32mpackage com.ht.hoteldelluna.controllers;[m
[32m+[m
[32m+[m[32mimport io.github.palexdev.materialfx.controls.MFXIconWrapper;[m
[32m+[m[32mimport io.github.palexdev.materialfx.controls.MFXRectangleToggleNode;[m
[32m+[m[32mimport io.github.palexdev.materialfx.controls.MFXScrollPane;[m
[32m+[m[32mimport com.ht.hoteldelluna.MFXResourcesLoader;[m
[32m+[m[32mimport io.github.palexdev.materialfx.utils.ScrollUtils;[m
[32m+[m[32mimport io.github.palexdev.materialfx.utils.ToggleButtonsUtil;[m
[32m+[m[32mimport io.github.palexdev.materialfx.utils.others.loader.MFXLoader;[m
[32m+[m[32mimport io.github.palexdev.materialfx.utils.others.loader.MFXLoaderBean;[m
[32m+[m[32mimport io.github.palexdev.mfxresources.fonts.MFXFontIcon;[m
[32m+[m[32mimport javafx.application.Platform;[m
[32m+[m[32mimport javafx.css.PseudoClass;[m
[32m+[m[32mimport javafx.fxml.FXML;[m
[32m+[m[32mimport javafx.fxml.Initializable;[m
[32m+[m[32mimport javafx.geometry.Bounds;[m
[32m+[m[32mimport javafx.geometry.Pos;[m
[32m+[m[32mimport javafx.scene.control.ToggleButton;[m
[32m+[m[32mimport javafx.scene.control.ToggleGroup;[m
[32m+[m[32mimport javafx.scene.image.Image;[m
[32m+[m[32mimport javafx.scene.image.ImageView;[m
[32m+[m[32mimport javafx.scene.input.MouseEvent;[m
[32m+[m[32mimport javafx.scene.layout.AnchorPane;[m
[32m+[m[32mimport javafx.scene.layout.HBox;[m
[32m+[m[32mimport javafx.scene.layout.StackPane;[m
[32m+[m[32mimport javafx.scene.layout.VBox;[m
[32m+[m[32mimport javafx.scene.shape.Circle;[m
[32m+[m[32mimport javafx.stage.Stage;[m
[32m+[m
[32m+[m[32mimport java.net.URL;[m
[32m+[m[32mimport java.util.List;[m
[32m+[m[32mimport java.util.ResourceBundle;[m
[32m+[m
[32m+[m[32mimport static com.ht.hoteldelluna.MFXResourcesLoader.loadURL;[m
[32m+[m
[32m+[m[32mpublic class MainController implements Initializable {[m
[32m+[m	[32mprivate final Stage stage;[m
[32m+[m	[32mprivate double xOffset;[m
[32m+[m	[32mprivate double yOffset;[m
[32m+[m	[32mprivate final ToggleGroup toggleGroup;[m
[32m+[m
[32m+[m	[32m@FXML[m
[32m+[m	[32mprivate HBox windowHeader;[m
[32m+[m
[32m+[m	[32m@FXML[m
[32m+[m	[32mprivate MFXFontIcon closeIcon;[m
[32m+[m
[32m+[m	[32m@FXML[m
[32m+[m	[32mprivate MFXFontIcon minimizeIcon;[m
[32m+[m
[32m+[m	[32m@FXML[m
[32m+[m	[32mprivate MFXFontIcon alwaysOnTopIcon;[m
[32m+[m
[32m+[m	[32m@FXML[m
[32m+[m	[32mprivate AnchorPane rootPane;[m
[32m+[m
[32m+[m	[32m@FXML[m
[32m+[m	[32mprivate MFXScrollPane scrollPane;[m
[32m+[m
[32m+[m	[32m@FXML[m
[32m+[m	[32mprivate VBox navBar;[m
[32m+[m
[32m+[m	[32m@FXML[m
[32m+[m	[32mprivate StackPane contentPane;[m
[32m+[m
[32m+[m	[32m@FXML[m
[32m+[m	[32mprivate StackPane logoContainer;[m
[32m+[m
[32m+[m	[32mpublic MainController(Stage stage) {[m
[32m+[m		[32mthis.stage = stage;[m
[32m+[m		[32mthis.toggleGroup = new ToggleGroup();[m
[32m+[m		[32mToggleButtonsUtil.addAlwaysOneSelectedSupport(toggleGroup);[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m@Override[m
[32m+[m	[32mpublic void initialize(URL location, ResourceBundle resources) {[m
[32m+[m		[32mcloseIcon.addEventHandler(MouseEvent.MOUSE_CLICKED, event -> Platform.exit());[m
[32m+[m		[32mminimizeIcon.addEventHandler(MouseEvent.MOUSE_CLICKED,[m
[32m+[m				[32mevent -> ((Stage) rootPane.getScene().getWindow()).setIconified(true));[m
[32m+[m		[32malwaysOnTopIcon.addEventHandler(MouseEvent.MOUSE_CLICKED, event -> {[m
[32m+[m			[32mboolean newVal = !stage.isAlwaysOnTop();[m
[32m+[m			[32malwaysOnTopIcon.pseudoClassStateChanged(PseudoClass.getPseudoClass("always-on-top"), newVal);[m
[32m+[m			[32mstage.setAlwaysOnTop(newVal);[m
[32m+[m		[32m});[m
[32m+[m
[32m+[m		[32mwindowHeader.setOnMousePressed(event -> {[m
[32m+[m			[32mxOffset = stage.getX() - event.getScreenX();[m
[32m+[m			[32myOffset = stage.getY() - event.getScreenY();[m
[32m+[m		[32m});[m
[32m+[m		[32mwindowHeader.setOnMouseDragged(event -> {[m
[32m+[m			[32mstage.setX(event.getScreenX() + xOffset);[m
[32m+[m			[32mstage.setY(event.getScreenY() + yOffset);[m
[32m+[m		[32m});[m
[32m+[m
[32m+[m		[32minitializeLoader();[m
[32m+[m
[32m+[m		[32mScrollUtils.addSmoothScrolling(scrollPane);[m
[32m+[m
[32m+[m		[32m// The only way to get a fucking smooth image in this shitty framework[m
[32m+[m		[32mImage image = new Image(MFXResourcesLoader.load("logo_alt.png"), 64, 64, true, true);[m
[32m+[m		[32mImageView logo = new ImageView(image);[m
[32m+[m		[32mCircle clip = new Circle(30);[m
[32m+[m		[32mlogo.setClip(clip);[m
[32m+[m		[32mlogoContainer.getChildren().add(logo);[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mprivate void initializeLoader() {[m
[32m+[m		[32mMFXLoader loader = new MFXLoader();[m
[32m+[m		[32mloader.addView(MFXLoaderBean.of("BUTTONS", loadURL("fxml/Buttons.fxml"))[m
[32m+[m				[32m.setBeanToNodeMapper(() -> createToggle("fas-circle-dot", "Buttons"))[m
[32m+[m				[32m.setDefaultRoot(true).get());[m
[32m+[m
[32m+[m		[32mloader.addView(MFXLoaderBean.of("TABLES", loadURL("fxml/TableViews.fxml"))[m
[32m+[m				[32m.setBeanToNodeMapper(() -> createToggle("fas-table", "Tables")).get());[m
[32m+[m
[32m+[m		[32mloader.addView(MFXLoaderBean.of("NOTIFICATIONS", loadURL("fxml/Notifications.fxml"))[m
[32m+[m				[32m.setBeanToNodeMapper(() -> createToggle("fas-bell", "Notifications"))[m
[32m+[m				[32m.setControllerFactory(c -> new NotificationsController(stage)).get());[m
[32m+[m
[32m+[m		[32mloader.setOnLoadedAction(beans -> {[m
[32m+[m			[32mList<ToggleButton> nodes = beans.stream()[m
[32m+[m					[32m.map(bean -> {[m
[32m+[m						[32mToggleButton toggle = (ToggleButton) bean.getBeanToNodeMapper().get();[m
[32m+[m						[32mtoggle.setOnAction(event -> contentPane.getChildren().setAll(bean.getRoot()));[m
[32m+[m						[32mif (bean.isDefaultView()) {[m
[32m+[m							[32mcontentPane.getChildren().setAll(bean.getRoot());[m
[32m+[m							[32mtoggle.setSelected(true);[m
[32m+[m						[32m}[m
[32m+[m						[32mreturn toggle;[m
[32m+[m					[32m})[m
[32m+[m					[32m.toList();[m
[32m+[m			[32mnavBar.getChildren().setAll(nodes);[m
[32m+[m		[32m});[m
[32m+[m		[32mloader.start();[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mprivate ToggleButton createToggle(String icon, String text) {[m
[32m+[m		[32mreturn createToggle(icon, text, 0);[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mprivate ToggleButton createToggle(String icon, String text, double rotate) {[m
[32m+[m		[32mMFXIconWrapper wrapper = new MFXIconWrapper(icon, 24, 32);[m
[32m+[m		[32mMFXRectangleToggleNode toggleNode = new MFXRectangleToggleNode(text, wrapper);[m
[32m+[m		[32mtoggleNode.setAlignment(Pos.CENTER_LEFT);[m
[32m+[m		[32mtoggleNode.setMaxWidth(Double.MAX_VALUE);[m
[32m+[m		[32mtoggleNode.setToggleGroup(toggleGroup);[m
[32m+[m		[32mif (rotate != 0)[m
[32m+[m			[32mwrapper.getIcon().setRotate(rotate);[m
[32m+[m		[32mreturn toggleNode;[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
