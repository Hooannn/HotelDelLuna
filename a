[32m+[m[32m        Platform.runLater(() -> {[m
[32m+[m[32m            dialogContent = MFXGenericDialogBuilder.build()[m
[32m+[m[32m                    .makeScrollable(true)[m
[32m+[m[32m                    .setShowMinimize(false)[m
[32m+[m[32m                    .setShowAlwaysOnTop(false)[m
[32m+[m[32m                    .get();[m
[32m+[m
[32m+[m[32m            dialog = MFXGenericDialogBuilder.build(dialogContent)[m
[32m+[m[32m                    .toStageDialogBuilder()[m
[32m+[m[32m                    .initOwner(stage)[m
[32m+[m[32m                    .initModality(Modality.APPLICATION_MODAL)[m
[32m+[m[32m                    .setDraggable(false)[m
[32m+[m[32m                    .setOwnerNode((Pane) stage.getScene().getRoot())[m
[32m+[m[32m                    .setScrimPriority(ScrimPriority.NODE)[m
[32m+[m[32m                    .setScrimOwner(true)[m
[32m+[m[32m                    .get();[m
[32m+[m
[32m+[m[32m            dialogContent.setMaxSize(stage.getMaxWidth(), stage.getMaxHeight());[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
