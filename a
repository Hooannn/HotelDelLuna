[32m+[m[32mpackage com.ht.hoteldelluna.backend.services;[m
[32m+[m
[32m+[m[32mimport com.ht.hoteldelluna.backend.Connection;[m
[32m+[m[32mimport com.ht.hoteldelluna.backend.Parser;[m
[32m+[m[32mimport com.ht.hoteldelluna.enums.ReservationStatus;[m
[32m+[m[32mimport com.ht.hoteldelluna.enums.RoomStatus;[m
[32m+[m[32mimport com.ht.hoteldelluna.models.Reservation;[m
[32m+[m[32mimport com.mongodb.client.MongoCollection;[m
[32m+[m[32mimport com.mongodb.client.result.DeleteResult;[m
[32m+[m[32mimport com.mongodb.client.result.InsertOneResult;[m
[32m+[m[32mimport com.mongodb.client.result.UpdateResult;[m
[32m+[m[32mimport org.bson.Document;[m
[32m+[m[32mimport org.bson.types.ObjectId;[m
[32m+[m
[32m+[m[32mimport java.util.ArrayList;[m
[32m+[m[32mimport java.util.List;[m
[32m+[m
[32m+[m[32mpublic class ReservationsService {[m
[32m+[m[32m    private final Parser parser = new Parser();[m
[32m+[m[32m    Connection dbConnection = Connection.shared;[m
[32m+[m[32m    MongoCollection<Document> roomCollection = dbConnection.getDatabase().getCollection("rooms");[m
[32m+[m[32m    MongoCollection<Document> reservationCollection = dbConnection.getDatabase().getCollection("reservations");[m
[32m+[m
[32m+[m[32m    public List<Reservation> getReservations() {[m
[32m+[m[32m        List<Document> documents = new ArrayList<>();[m
[32m+[m[32m        reservationCollection.find().into(documents);[m
[32m+[m[32m        documents.forEach(this::populateReservationDocument);[m
[32m+[m[32m        return parser.fromDocuments(documents, Reservation.class);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public List<Reservation> getOpeningReservations() {[m
[32m+[m[32m        List<Document> documents = new ArrayList<>();[m
[32m+[m[32m        reservationCollection.find(new Document("status", ReservationStatus.OPENING)).into(documents);[m
[32m+[m[32m        documents.forEach(this::populateReservationDocument);[m
[32m+[m[32m        return parser.fromDocuments(documents, Reservation.class);[m
[32m+[m[32m    }[m
[32m+[m[32m    public Reservation getReservationById(String reservationId) {[m
[32m+[m[32m        Document doc = reservationCollection.find(new Document("_id", new ObjectId(reservationId))).first();[m
[32m+[m[32m        assert doc != null;[m
[32m+[m[32m        populateReservationDocument(doc);[m
[32m+[m[32m        return parser.fromDocument(doc, Reservation.class);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public Reservation getReservationRoomId(String roomId) {[m
[32m+[m[32m        Document doc = reservationCollection.find(new Document("room", new ObjectId(roomId))).first();[m
[32m+[m[32m        assert doc != null;[m
[32m+[m[32m        populateReservationDocument(doc);[m
[32m+[m[32m        return parser.fromDocument(doc, Reservation.class);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public UpdateResult closeReservation(String reservationId, String roomId) {[m
[32m+[m[32m        RoomsService roomsService = new RoomsService();[m
[32m+[m[32m        UpdateResult res = reservationCollection.updateOne(new Document("_id", new ObjectId(reservationId)), new Document("$set", new Document("status", ReservationStatus.CLOSED)));[m
[32m+[m[32m        roomsService.updateRoomStatus(roomId, RoomStatus.AVAILABLE);[m
[32m+[m[32m        return res;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public UpdateResult updateNote(String reservationId, String updatedNote) {[m
[32m+[m[32m        return reservationCollection.updateOne(new Document("_id", new ObjectId(reservationId)), new Document("$set", new Document("note", updatedNote)));[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public InsertOneResult addReservation(Reservation reservation, String roomId) {[m
[32m+[m[32m        RoomsService roomsService = new RoomsService();[m
[32m+[m[32m        Document document = parser.toDocument(reservation);[m
[32m+[m[32m        document.put("room", new ObjectId(roomId));[m
[32m+[m[32m        roomsService.updateRoomStatus(roomId, RoomStatus.OCCUPIED);[m
[32m+[m[32m        return reservationCollection.insertOne(document);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public DeleteResult deleteReservation(String reservationId) {[m
[32m+[m[32m        return reservationCollection.deleteOne(new Document("_id", new ObjectId(reservationId)));[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private void populateReservationDocument(Document document) {[m
[32m+[m[32m        ObjectId roomId = document.getObjectId("room");[m
[32m+[m[32m        Document room = roomCollection.find(new Document("_id", roomId)).first();[m
[32m+[m[32m        document.put("room", room);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
