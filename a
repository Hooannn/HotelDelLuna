[32m+[m[32m    public void showTypeEmptyNotification() {[m
[32m+[m[32m        Alert alert = new Alert(Alert.AlertType.ERROR);[m
[32m+[m[32m        alert.setTitle("Type Empty");[m
[32m+[m[32m        alert.setHeaderText(null);[m
[32m+[m[32m        alert.setContentText("Please choose type.");[m
[32m+[m[32m        alert.showAndWait();[m
[32m+[m[32m    }[m
[32m+[m[32m    @FXML[m
[32m+[m[32m    public  void checkOK (ActionEvent event) {[m
[32m+[m[32m        String name = nameOfRoom.getText();[m
[32m+[m[32m        RoomsService roomsService = new RoomsService();[m
[32m+[m[32m        //check nameofRoom in Room database has been existed or not. If existed then show a dialog to notify[m
[32m+[m[32m        if (roomsService.checkNameofRoom(name)) {[m
[32m+[m[32m            showRoomExistsNotification();[m
[32m+[m[32m            return;[m
[32m+[m[32m        }[m
[32m+[m[32m        else if (floor.getSelectionModel().getSelectedItem() == null) {[m
[32m+[m[32m            showFloorEmptyNotification();[m
[32m+[m[32m            return;[m
[32m+[m[32m        }[m
[32m+[m[32m        else if (type.getSelectionModel().getSelectedItem() == null) {[m
[32m+[m[32m            showTypeEmptyNotification();[m
[32m+[m[32m            return;[m
[32m+[m[32m        }[m
[32m+[m[32m        RoomTypesService roomTypesService = new RoomTypesService();[m
[32m+[m[32m        RoomType roomType = roomTypesService.getRoomTypeByName(type.getText());[m
[32m+[m[32m        String t = type.getText();[m
[32m+[m[32m        String floorText = floor.getText();[m
[32m+[m[32m        int intValue = Integer.parseInt(floorText);[m
[32m+[m[32m        FloorsService floorsService = new FloorsService();[m
[32m+[m[32m        Floor floorNumber = floorsService.getFloorByNum(intValue);[m
[32m+[m[32m        Double overnightPrice = 0.0;[m
[32m+[m
[32m+[m[32m// public Room(String name, RoomType type, Floor floor, double overnightPrice, RoomStatus status)[m
[32m+[m
[32m+[m[32m        Room room = new Room(name, roomType, floorNumber, overnightPrice, RoomStatus.AVAILABLE);[m
[32m+[m[32m        roomsService.addRoom(room, roomType.getId().toString(), floorNumber.getId().toString());[m
[32m+[m[32m        Stage stage = (Stage) nameOfRoom.getScene().getWindow();[m
[32m+[m[32m        stage.close();[m
[32m+[m
[32m+[m[32m    }[m
[32m+[m[32m    public void checkCancel (ActionEvent event) {[m
[32m+[m[32m        Stage stage = (Stage) nameOfRoom.getScene().getWindow();[m
[32m+[m[32m        stage.close();[m
[32m+[m[32m    }[m
