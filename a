[32m+[m
[32m+[m[32mpackage com.mycompany.app;[m
[32m+[m
[32m+[m[32mimport com.esri.arcgisruntime.ArcGISRuntimeEnvironment;[m
[32m+[m[32mimport com.esri.arcgisruntime.mapping.BasemapStyle;[m
[32m+[m[32mimport com.esri.arcgisruntime.mapping.ArcGISMap;[m
[32m+[m[32mimport com.esri.arcgisruntime.mapping.view.MapView;[m
[32m+[m[32mimport javafx.application.Application;[m
[32m+[m[32mimport javafx.scene.Scene;[m
[32m+[m[32mimport javafx.scene.layout.StackPane;[m
[32m+[m[32mimport javafx.stage.Stage;[m
[32m+[m
[32m+[m[32mpublic class App extends Application {[m
[32m+[m
[32m+[m[32m    private MapView mapView;[m
[32m+[m
[32m+[m[32m    public static void main(String[] args) {[m
[32m+[m
[32m+[m[32m        Application.launch(args);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void start(Stage stage) {[m
[32m+[m
[32m+[m[32m        // set the title and size of the stage and show it[m
[32m+[m[32m        stage.setTitle("My Map App");[m
[32m+[m[32m        stage.setWidth(800);[m
[32m+[m[32m        stage.setHeight(700);[m
[32m+[m[32m        stage.show();[m
[32m+[m
[32m+[m[32m        // create a JavaFX scene with a stack pane as the root node and add it to the scene[m
[32m+[m[32m        StackPane stackPane = new StackPane();[m
[32m+[m[32m        Scene scene = new Scene(stackPane);[m
[32m+[m[32m        stage.setScene(scene);[m
[32m+[m
[32m+[m[32m        // Note: it is not best practice to store API keys in source code.[m
[32m+[m[32m        // An API key is required to enable access to services, web maps, and web scenes hosted in ArcGIS Online.[m
[32m+[m[32m        // If you haven't already, go to your developer dashboard to get your API key.[m
[32m+[m[32m        // Please refer to https://developers.arcgis.com/java/get-started/ for more information[m
[32m+[m[32m        String yourApiKey = "YOUR_API_KEY";[m
[32m+[m[32m        ArcGISRuntimeEnvironment.setApiKey(yourApiKey);[m
[32m+[m
[32m+[m[32m        // create a MapView to display the map and add it to the stack pane[m
[32m+[m[32m        mapView = new MapView();[m
[32m+[m[32m        stackPane.getChildren().add(mapView);[m
[32m+[m
[32m+[m[32m        // create an ArcGISMap with an imagery basemap[m
[32m+[m[32m        ArcGISMap map = new ArcGISMap(BasemapStyle.ARCGIS_IMAGERY);[m
[32m+[m
[32m+[m[32m        // display the map by setting the map on the map view[m
[32m+[m[32m        mapView.setMap(map);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Stops and releases all resources used in application.[m
[32m+[m[32m     */[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void stop() {[m
[32m+[m
[32m+[m[32m        if (mapView != null) {[m
[32m+[m[32m            mapView.dispose();[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[1mdiff --git a/src/main/java/module-info.java b/src/main/java/module-info.java[m
[1mnew file mode 100644[m
[1mindex 0000000..24d021f[m
[1m--- /dev/null[m
[1m+++ b/src/main/java/module-info.java[m
