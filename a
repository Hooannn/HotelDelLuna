[32m+[m[32m    private void updateRoom(javafx.event.ActionEvent event) throws Exception {[m
[32m+[m
[32m+[m[32m        if ( floor.getSelectedItem() == null || type.getSelectedItem() == null) {[m
[32m+[m[32m            System.out.println("No update");[m
[32m+[m[32m            return;[m
[32m+[m[32m        }[m
[32m+[m[32m        else {[m
[32m+[m
[32m+[m[32m            RoomsService roomsService = new RoomsService();[m
[32m+[m[32m            String roomName = name.getText();[m
[32m+[m[32m            Floor valueFloor= floor.getSelectedItem();[m
[32m+[m[32m            RoomType valueRoomType = type.getSelectedItem();[m
[32m+[m[32m            roomsService.updateRoom(String.valueOf(room.getId()), roomName, String.valueOf(valueRoomType.getId()), String.valueOf(valueFloor.getId()));[m
[32m+[m[32m        }[m
[32m+[m[32m        // Update room[m
[32m+[m
[32m+[m[32m    }[m
