[32m+[m[32mpackage com.ht.hoteldelluna.backend;[m
[32m+[m[32mimport com.google.gson.*;[m
[32m+[m[32mimport org.bson.types.ObjectId;[m
[32m+[m
[32m+[m[32mpublic class ObjectIdDeserializer implements JsonDeserializer<ObjectId> {[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public ObjectId deserialize(JsonElement json, java.lang.reflect.Type typeOfT, JsonDeserializationContext context) throws JsonParseException {[m
[32m+[m[32m        JsonObject jsonObject = json.getAsJsonObject();[m
[32m+[m[32m        String oid = jsonObject.getAsJsonPrimitive("$oid").getAsString();[m
[32m+[m[32m        return new ObjectId(oid);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
