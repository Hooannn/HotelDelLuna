[32m+[m[32mpackage com.ht.hoteldelluna.backend.services;[m
[32m+[m
[32m+[m[32mimport com.ht.hoteldelluna.backend.Connection;[m
[32m+[m[32mimport com.ht.hoteldelluna.backend.Parser;[m
[32m+[m[32mimport com.ht.hoteldelluna.models.Floor;[m
[32m+[m[32mimport com.mongodb.client.MongoCollection;[m
[32m+[m[32mimport com.mongodb.client.result.DeleteResult;[m
[32m+[m[32mimport com.mongodb.client.result.InsertManyResult;[m
[32m+[m[32mimport com.mongodb.client.result.InsertOneResult;[m
[32m+[m[32mimport org.bson.Document;[m
[32m+[m[32mimport org.bson.types.ObjectId;[m
[32m+[m
[32m+[m[32mimport java.util.ArrayList;[m
[32m+[m[32mimport java.util.List;[m
[32m+[m
[32m+[m[32mpublic class FloorServices {[m
[32m+[m[32m    private final Parser parser = new Parser();[m
[32m+[m[32m    Connection dbConnection = Connection.shared;[m
[32m+[m[32m    MongoCollection<Document> floorCollection = dbConnection.getDatabase().getCollection("floors");[m
[32m+[m
[32m+[m[32m    public List<Floor> getFloors() {[m
[32m+[m[32m        List<Document> documents = new ArrayList<>();[m
[32m+[m[32m        floorCollection.find().into(documents);[m
[32m+[m[32m        return parser.fromDocuments(documents, Floor.class);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public Floor getFloorById(String floorId) {[m
[32m+[m[32m        Document doc = floorCollection.find(new Document("_id", new ObjectId(floorId))).first();[m
[32m+[m[32m        assert doc != null;[m
[32m+[m[32m        return parser.fromDocument(doc, Floor.class);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public InsertOneResult addFloor(Floor floor) {[m
[32m+[m[32m        Document document = parser.toDocument(floor);[m
[32m+[m[32m        return floorCollection.insertOne(document);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public DeleteResult deleteFloor(String floorId) {[m
[32m+[m[32m        return floorCollection.deleteOne(new Document("_id", new ObjectId(floorId)));[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public InsertManyResult addMockFloors() {[m
[32m+[m[32m        List<Floor> floors = new ArrayList<>();[m
[32m+[m[32m        floors.add(new Floor(1));[m
[32m+[m[32m        floors.add(new Floor(2));[m
[32m+[m[32m        floors.add(new Floor(3));[m
[32m+[m[32m        List<Document> documents = parser.toDocuments(floors);[m
[32m+[m[32m        return floorCollection.insertMany(documents);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
