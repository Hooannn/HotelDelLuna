[32m+[m[32mpackage com.ht.hoteldelluna.controllers.main.RoomManager;[m
[32m+[m
[32m+[m[32mimport com.ht.hoteldelluna.MFXResourcesLoader;[m
[32m+[m[32mimport com.ht.hoteldelluna.backend.services.FloorsService;[m
[32m+[m[32mimport com.ht.hoteldelluna.backend.services.RoomsService;[m
[32m+[m[32mimport com.ht.hoteldelluna.models.Floor;[m
[32m+[m[32mimport com.ht.hoteldelluna.models.Room;[m
[32m+[m[32mimport io.github.palexdev.materialfx.controls.MFXComboBox;[m
[32m+[m[32mimport javafx.collections.FXCollections;[m
[32m+[m[32mimport javafx.fxml.FXML;[m
[32m+[m[32mimport javafx.fxml.FXMLLoader;[m
[32m+[m[32mimport javafx.fxml.Initializable;[m
[32m+[m[32mimport javafx.scene.layout.FlowPane;[m
[32m+[m[32mimport javafx.stage.Stage;[m
[32m+[m
[32m+[m[32mimport java.io.IOException;[m
[32m+[m[32mimport java.net.URL;[m
[32m+[m[32mimport java.util.List;[m
[32m+[m[32mimport java.util.ResourceBundle;[m
[32m+[m
[32m+[m[32mpublic class RoomManagerController implements Initializable {[m
[32m+[m[32m    @FXML[m
[32m+[m[32m    private FlowPane roomFlowPane;[m
[32m+[m[32m    @FXML[m
[32m+[m[32m    private MFXComboBox<Floor> floorsSelection;[m
[32m+[m[32m    private final RoomsService roomsService = new RoomsService();[m
[32m+[m[32m    private final FloorsService floorsService = new FloorsService();[m
[32m+[m
[32m+[m[32m    private List<Room> rooms;[m
[32m+[m[32m    private List<Floor> floors;[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void initialize(URL url, ResourceBundle resourceBundle) {[m
[32m+[m[32m        setupFloorsSelection();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public RoomManagerController(Stage stage) {[m
[32m+[m[32m        floors = floorsService.getFloors();[m
[32m+[m[32m        rooms = roomsService.getRooms();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private void setupFloorsSelection() {[m
[32m+[m[32m        floorsSelection.setItems(FXCollections.observableList(floors));[m
[32m+[m[32m        floorsSelection.valueProperty().addListener((observable, oldValue, newValue) -> setupRoomCards(newValue.getNum()));[m
[32m+[m[32m        floorsSelection.selectFirst();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private void setupRoomCards(int floor) {[m
[32m+[m[32m        roomFlowPane.getChildren().clear();[m
[32m+[m[32m        List<Room> renderRooms = rooms.stream().filter(room -> room.getFloor().getNum() == floor).toList();[m
[32m+[m[32m        renderRooms.forEach(room -> {[m
[32m+[m[32m            FXMLLoader loader = new FXMLLoader(MFXResourcesLoader.loadURL("fxml/main/RoomManager/RoomCard.fxml"));[m
[32m+[m[32m            loader.setControllerFactory(c -> new RoomCardController(room));[m
[32m+[m[32m            try {[m
[32m+[m[32m                roomFlowPane.getChildren().add(loader.load());[m
[32m+[m[32m            } catch (IOException e) {[m
[32m+[m[32m                throw new RuntimeException(e);[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
