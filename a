[32m+[m[32mimport javafx.beans.property.StringProperty;[m
[32m+[m[32mimport javafx.event.ActionEvent;[m
[32m+[m[32mimport javafx.fxml.FXML;[m
[32m+[m[32mimport javafx.geometry.Pos;[m
[32m+[m[32mimport javafx.scene.control.Label;[m
[32m+[m[32mimport javafx.scene.layout.BorderPane;[m
[32m+[m[32mimport javafx.scene.layout.HBox;[m
[32m+[m[32mimport javafx.scene.layout.Priority;[m
[32m+[m[32mimport javafx.scene.layout.StackPane;[m
[32m+[m[32mimport javafx.stage.Stage;[m
[32m+[m
[32m+[m[32mpublic class NotificationsController {[m
[32m+[m
[32m+[m	[32mpublic NotificationsController(Stage stage) {[m
[32m+[m		[32mPlatform.runLater(() -> {[m
[32m+[m			[32mMFXNotificationSystem.instance().initOwner(stage);[m
[32m+[m			[32mMFXNotificationCenterSystem.instance().initOwner(stage);[m
[32m+[m
[32m+[m			[32mMFXNotificationCenter center = MFXNotificationCenterSystem.instance().getCenter();[m
[32m+[m			[32mcenter.setCellFactory(notification -> new MFXNotificationCell(center, notification) {[m
[32m+[m				[32m{[m
[32m+[m					[32msetPrefHeight(400);[m
[32m+[m				[32m}[m
[32m+[m			[32m});[m
[32m+[m		[32m});[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m@FXML[m
[32m+[m	[32mvoid showTopLeft(ActionEvent event) {[m
[32m+[m		[32mMFXNotificationSystem.instance()[m
[32m+[m				[32m.setPosition(NotificationPos.TOP_LEFT)[m
[32m+[m				[32m.publish(createNotification());[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m@FXML[m
[32m+[m	[32mvoid showTopCenter(ActionEvent event) {[m
[32m+[m		[32mMFXNotificationSystem.instance()[m
[32m+[m				[32m.setPosition(NotificationPos.TOP_CENTER)[m
[32m+[m				[32m.publish(createNotification());[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m@FXML[m
[32m+[m	[32mvoid showTopRight(ActionEvent event) {[m
[32m+[m		[32mMFXNotificationSystem.instance()[m
[32m+[m				[32m.setPosition(NotificationPos.TOP_RIGHT)[m
[32m+[m				[32m.publish(createNotification());[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m@FXML[m
[32m+[m	[32mvoid showBottomLeft(ActionEvent event) {[m
[32m+[m		[32mMFXNotificationSystem.instance()[m
[32m+[m				[32m.setPosition(NotificationPos.BOTTOM_LEFT)[m
[32m+[m				[32m.publish(createNotification());[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m@FXML[m
[32m+[m	[32mvoid showBottomCenter(ActionEvent event) {[m
[32m+[m		[32mMFXNotificationSystem.instance()[m
[32m+[m				[32m.setPosition(NotificationPos.BOTTOM_LEFT)[m
[32m+[m				[32m.publish(createNotification());[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m@FXML[m
[32m+[m	[32mvoid showBottomRight(ActionEvent event) {[m
[32m+[m		[32mMFXNotificationSystem.instance()[m
[32m+[m				[32m.setPosition(NotificationPos.BOTTOM_RIGHT)[m
[32m+[m				[32m.publish(createNotification());[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m@FXML[m
[32m+[m	[32mvoid showTopLeftNC(ActionEvent event) {[m
[32m+[m		[32mMFXNotificationCenterSystem.instance()[m
[32m+[m				[32m.setPosition(NotificationPos.TOP_LEFT)[m
[32m+[m				[32m.publish(createNotification());[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m@FXML[m
[32m+[m	[32mvoid showTopCenterNC(ActionEvent event) {[m
[32m+[m		[32mMFXNotificationCenterSystem.instance()[m
[32m+[m				[32m.setPosition(NotificationPos.TOP_CENTER)[m
[32m+[m				[32m.publish(createNotification());[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m@FXML[m
[32m+[m	[32mvoid showTopRightNC(ActionEvent event) {[m
[32m+[m		[32mMFXNotificationCenterSystem.instance()[m
[32m+[m				[32m.setPosition(NotificationPos.TOP_RIGHT)[m
[32m+[m				[32m.publish(createNotification());[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m@FXML[m
[32m+[m	[32mvoid showBottomLeftNC(ActionEvent event) {[m
[32m+[m		[32mMFXNotificationCenterSystem.instance()[m
[32m+[m				[32m.setPosition(NotificationPos.BOTTOM_LEFT)[m
[32m+[m				[32m.publish(createNotification());[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m@FXML[m
[32m+[m	[32mvoid showBottomCenterNC(ActionEvent event) {[m
[32m+[m		[32mMFXNotificationCenterSystem.instance()[m
[32m+[m				[32m.setPosition(NotificationPos.BOTTOM_CENTER)[m
[32m+[m				[32m.publish(createNotification());[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32m@FXML[m
[32m+[m	[32mvoid showBottomRightNC(ActionEvent event) {[m
[32m+[m		[32mMFXNotificationCenterSystem.instance()[m
[32m+[m				[32m.setPosition(NotificationPos.BOTTOM_RIGHT)[m
[32m+[m				[32m.publish(createNotification());[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mprivate INotification createNotification() {[m
[32m+[m		[32mExampleNotification notification = new ExampleNotification();[m
[32m+[m		[32mnotification.setContentText(RandomUtils.randFromArray(Model.randomText));[m
[32m+[m		[32mreturn notification;[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mprivate static class ExampleNotification extends MFXSimpleNotification {[m
[32m+[m		[32mprivate final StringProperty headerText = new SimpleStringProperty("Notification Header");[m
[32m+[m		[32mprivate final StringProperty contentText = new SimpleStringProperty();[m
[32m+[m
[32m+[m		[32mpublic ExampleNotification() {[m
[32m+[m
[32m+[m			[32mMFXFontIcon fi = new MFXFontIcon();[m
[32m+[m			[32mIconDescriptor desc = RandomUtils.randFromArray(Model.notificationsIcons);[m
[32m+[m			[32mif (desc instanceof FontAwesomeRegular) {[m
[32m+[m				[32mfi.setIconsProvider(IconsProviders.FONTAWESOME_REGULAR);[m
[32m+[m			[32m} else if (desc instanceof FontAwesomeBrands) {[m
[32m+[m				[32mfi.setIconsProvider(IconsProviders.FONTAWESOME_BRANDS);[m
[32m+[m			[32m}[m
[32m+[m			[32mfi.setDescription(desc.getDescription());[m
[32m+[m			[32mfi.setSize(16);[m
[32m+[m			[32mMFXIconWrapper icon = new MFXIconWrapper(fi, 32);[m
[32m+[m			[32mLabel headerLabel = new Label();[m
[32m+[m			[32mheaderLabel.textProperty().bind(headerText);[m
[32m+[m			[32mMFXIconWrapper readIcon = new MFXIconWrapper("fas-eye", 16, 32);[m
[32m+[m			[32m((MFXFontIcon) readIcon.getIcon()).descriptionProperty().bind(Bindings.createStringBinding([m
[32m+[m					[32m() -> (getState() == NotificationState.READ) ? "fas-eye" : "fas-eye-slash",[m
[32m+[m					[32mnotificationStateProperty()));[m
[32m+[m			[32mStackPane.setAlignment(readIcon, Pos.CENTER_RIGHT);[m
[32m+[m			[32mStackPane placeHolder = new StackPane(readIcon);[m
[32m+[m			[32mplaceHolder.setMaxWidth(Double.MAX_VALUE);[m
[32m+[m			[32mHBox.setHgrow(placeHolder, Priority.ALWAYS);[m
[32m+[m			[32mHBox header = new HBox(10, icon, headerLabel, placeHolder);[m
[32m+[m			[32mheader.setAlignment(Pos.CENTER_LEFT);[m
[32m+[m			[32mheader.setPadding(InsetsFactory.of(5, 0, 5, 0));[m
[32m+[m			[32mheader.setMaxWidth(Double.MAX_VALUE);[m
[32m+[m
[32m+[m			[32mLabel contentLabel = new Label();[m
[32m+[m			[32mcontentLabel.getStyleClass().add("content");[m
[32m+[m			[32mcontentLabel.textProperty().bind(contentText);[m
[32m+[m			[32mcontentLabel.setWrapText(true);[m
[32m+[m			[32mcontentLabel.setMaxSize(Double.MAX_VALUE, Double.MAX_VALUE);[m
[32m+[m			[32mcontentLabel.setAlignment(Pos.TOP_LEFT);[m
[32m+[m
[32m+[m			[32mMFXButton action1 = new MFXButton("Action 1");[m
[32m+[m			[32mMFXButton action2 = new MFXButton("Action 2");[m
[32m+[m			[32mHBox actionsBar = new HBox(15, action1, action2);[m
[32m+[m			[32mactionsBar.getStyleClass().add("actions-bar");[m
[32m+[m			[32mactionsBar.setAlignment(Pos.CENTER_RIGHT);[m
[32m+[m			[32mactionsBar.setPadding(InsetsFactory.all(5));[m
[32m+[m
[32m+[m			[32mBorderPane container = new BorderPane();[m
[32m+[m			[32mcontainer.getStyleClass().add("notification");[m
[32m+[m			[32mcontainer.setTop(header);[m
[32m+[m			[32mcontainer.setCenter(contentLabel);[m
[32m+[m			[32mcontainer.setBottom(actionsBar);[m
[32m+[m			[32mcontainer.getStylesheets().add(MFXResourcesLoader.load("css/Notification.css"));[m
[32m+[m			[32mcontainer.setMinHeight(200);[m
[32m+[m			[32mcontainer.setMaxWidth(400);[m
[32m+[m
[32m+[m			[32msetContent(container);[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mpublic String getHeaderText() {[m
[32m+[m			[32mreturn headerText.get();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mpublic StringProperty headerTextProperty() {[m
[32m+[m			[32mreturn headerText;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mpublic void setHeaderText(String headerText) {[m
[32m+[m			[32mthis.headerText.set(headerText);[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mpublic String getContentText() {[m
[32m+[m			[32mreturn contentText.get();[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mpublic StringProperty contentTextProperty() {[m
[32m+[m			[32mreturn contentText;[m
[32m+[m		[32m}[m
[32m+[m
[32m+[m		[32mpublic void setContentText(String contentText) {[m
[32m+[m			[32mthis.contentText.set(contentText);[m
[32m+[m		[32m}[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
