[32m+[m[32m    private void signOut() {[m
[32m+[m[32m        signOutBtn.getScene().getWindow().hide();[m
[32m+[m
[32m+[m[32m        try {[m
[32m+[m[32m            appState.setAuthUser(null);[m
[32m+[m[32m            Stage authStage = new Stage();[m
[32m+[m[32m            FXMLLoader loader = new FXMLLoader(MFXResourcesLoader.loadURL("fxml/Authentication.fxml"));[m
[32m+[m[32m            loader.setControllerFactory(c -> new AuthController(authStage));[m
[32m+[m[32m            Parent root = loader.load();[m
[32m+[m[32m            Scene scene = new Scene(root);[m
[32m+[m[32m            MFXThemeManager.addOn(scene, Themes.DEFAULT, Themes.LEGACY);[m
[32m+[m[32m            scene.setFill(Color.TRANSPARENT);[m
[32m+[m[32m            authStage.initStyle(StageStyle.TRANSPARENT);[m
[32m+[m[32m            authStage.setScene(scene);[m
[32m+[m[32m            authStage.setTitle("Hotel Del Luna");[m
[32m+[m[32m            authStage.show();[m
[32m+[m[32m        } catch (Exception e) {[m
[32m+[m[32m            e.printStackTrace();[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private void setupDialog() {[m
[32m+[m[32m        Platform.runLater(() -> {[m
[32m+[m[32m            dialogContent = MFXGenericDialogBuilder.build()[m
[32m+[m[32m                    .makeScrollable(true)[m
[32m+[m[32m                    .setShowMinimize(false)[m
[32m+[m[32m                    .setShowAlwaysOnTop(false)[m
[32m+[m[32m                    .get();[m
[32m+[m
[32m+[m[32m            dialog = MFXGenericDialogBuilder.build(dialogContent)[m
[32m+[m[32m                    .toStageDialogBuilder()[m
[32m+[m[32m                    .initOwner(stage)[m
[32m+[m[32m                    .initModality(Modality.APPLICATION_MODAL)[m
[32m+[m[32m                    .setDraggable(false)[m
[32m+[m[32m                    .setOwnerNode((Pane) stage.getScene().getRoot())[m
[32m+[m[32m                    .setScrimPriority(ScrimPriority.NODE)[m
[32m+[m[32m                    .setScrimOwner(true)[m
[32m+[m[32m                    .get();[m
[32m+[m
[32m+[m[32m            dialogContent.addActions([m
[32m+[m[32m                    Map.entry(new MFXButton("C√≥"), event -> signOut()),[m
[32m+[m[32m                    Map.entry(new MFXButton("Kh√¥ng"), event -> dialog.close())[m
[32m+[m[32m            );[m
[32m+[m
[32m+[m[32m            dialogContent.setMaxSize(stage.getMaxWidth(), stage.getMaxHeight());[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
