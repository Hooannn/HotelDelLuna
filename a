[32m+[m[32mpackage com.ht.hoteldelluna.controllers;[m
[32m+[m
[32m+[m[32mimport io.github.palexdev.materialfx.controls.MFXPaginatedTableView;[m
[32m+[m[32mimport io.github.palexdev.materialfx.controls.MFXTableColumn;[m
[32m+[m[32mimport io.github.palexdev.materialfx.controls.MFXTableView;[m
[32m+[m[32mimport io.github.palexdev.materialfx.controls.cell.MFXTableRowCell;[m
[32m+[m[32mimport com.ht.hoteldelluna.models.Device;[m
[32m+[m[32mimport com.ht.hoteldelluna.models.Model;[m
[32m+[m[32mimport com.ht.hoteldelluna.models.Person;[m
[32m+[m[32mimport io.github.palexdev.materialfx.filter.EnumFilter;[m
[32m+[m[32mimport io.github.palexdev.materialfx.filter.IntegerFilter;[m
[32m+[m[32mimport io.github.palexdev.materialfx.filter.StringFilter;[m
[32m+[m[32mimport io.github.palexdev.materialfx.utils.others.observables.When;[m
[32m+[m[32mimport javafx.fxml.FXML;[m
[32m+[m[32mimport javafx.fxml.Initializable;[m
[32m+[m[32mimport javafx.geometry.Pos;[m
[32m+[m
[32m+[m[32mimport java.net.URL;[m
[32m+[m[32mimport java.util.Comparator;[m
[32m+[m[32mimport java.util.ResourceBundle;[m
[32m+[m
[32m+[m[32mpublic class TableViewsController implements Initializable {[m
[32m+[m
[32m+[m	[32m@FXML[m
[32m+[m	[32mprivate MFXTableView<Person> table;[m
[32m+[m
[32m+[m	[32m@FXML[m
[32m+[m	[32mprivate MFXPaginatedTableView<Device> paginated;[m
[32m+[m
[32m+[m	[32m@Override[m
[32m+[m	[32mpublic void initialize(URL location, ResourceBundle resources) {[m
[32m+[m		[32msetupTable();[m
[32m+[m		[32msetupPaginated();[m
[32m+[m
[32m+[m		[32mtable.autosizeColumnsOnInitialization();[m
[32m+[m		[32mpaginated.autosizeColumnsOnInitialization();[m
[32m+[m
[32m+[m		[32mWhen.onChanged(paginated.currentPageProperty())[m
[32m+[m				[32m.then((oldValue, newValue) -> paginated.autosizeColumns())[m
[32m+[m				[32m.listen();[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mprivate void setupTable() {[m
[32m+[m		[32mMFXTableColumn<Person> nameColumn = new MFXTableColumn<>("Name", true, Comparator.comparing(Person::getName));[m
[32m+[m		[32mMFXTableColumn<Person> surnameColumn = new MFXTableColumn<>("Surname", true,[m
[32m+[m				[32mComparator.comparing(Person::getSurname));[m
[32m+[m		[32mMFXTableColumn<Person> ageColumn = new MFXTableColumn<>("Age", true, Comparator.comparing(Person::getAge));[m
[32m+[m
[32m+[m		[32mnameColumn.setRowCellFactory(person -> new MFXTableRowCell<>(Person::getName));[m
[32m+[m		[32msurnameColumn.setRowCellFactory(person -> new MFXTableRowCell<>(Person::getSurname));[m
[32m+[m		[32mageColumn.setRowCellFactory(person -> new MFXTableRowCell<>(Person::getAge) {[m
[32m+[m			[32m{[m
[32m+[m				[32msetAlignment(Pos.CENTER_RIGHT);[m
[32m+[m			[32m}[m
[32m+[m		[32m});[m
[32m+[m		[32mageColumn.setAlignment(Pos.CENTER_RIGHT);[m
[32m+[m
[32m+[m		[32mtable.getTableColumns().addAll(nameColumn, surnameColumn, ageColumn);[m
[32m+[m		[32mtable.getFilters().addAll([m
[32m+[m				[32mnew StringFilter<>("Name", Person::getName),[m
[32m+[m				[32mnew StringFilter<>("Surname", Person::getSurname),[m
[32m+[m				[32mnew IntegerFilter<>("Age", Person::getAge));[m
[32m+[m		[32mtable.setItems(Model.people);[m
[32m+[m	[32m}[m
[32m+[m
[32m+[m	[32mprivate void setupPaginated() {[m
[32m+[m		[32mMFXTableColumn<Device> idColumn = new MFXTableColumn<>("ID", false, Comparator.comparing(Device::getID));[m
[32m+[m		[32mMFXTableColumn<Device> nameColumn = new MFXTableColumn<>("Name", false, Comparator.comparing(Device::getName));[m
[32m+[m		[32mMFXTableColumn<Device> ipColumn = new MFXTableColumn<>("IP", false, Comparator.comparing(Device::getIP));[m
[32m+[m		[32mMFXTableColumn<Device> ownerColumn = new MFXTableColumn<>("Owner", false,[m
[32m+[m				[32mComparator.comparing(Device::getOwner));[m
[32m+[m		[32mMFXTableColumn<Device> stateColumn = new MFXTableColumn<>("State", false,[m
[32m+[m				[32mComparator.comparing(Device::getState));[m
[32m+[m
[32m+[m		[32midColumn.setRowCellFactory(device -> new MFXTableRowCell<>(Device::getID));[m
[32m+[m		[32mnameColumn.setRowCellFactory(device -> new MFXTableRowCell<>(Device::getName));[m
[32m+[m		[32mipColumn.setRowCellFactory(device -> new MFXTableRowCell<>(Device::getIP) {[m
[32m+[m			[32m{[m
[32m+[m				[32msetAlignment(Pos.CENTER_RIGHT);[m
[32m+[m			[32m}[m
[32m+[m		[32m});[m
[32m+[m		[32mownerColumn.setRowCellFactory(device -> new MFXTableRowCell<>(Device::getOwner));[m
[32m+[m		[32mstateColumn.setRowCellFactory(device -> new MFXTableRowCell<>(Device::getState));[m
[32m+[m		[32mipColumn.setAlignment(Pos.CENTER_RIGHT);[m
[32m+[m
[32m+[m		[32mpaginated.getTableColumns().addAll(idColumn, nameColumn, ipColumn, ownerColumn, stateColumn);[m
[32m+[m		[32mpaginated.getFilters().addAll([m
[32m+[m				[32mnew IntegerFilter<>("ID", Device::getID),[m
[32m+[m				[32mnew StringFilter<>("Name", Device::getName),[m
[32m+[m				[32mnew StringFilter<>("IP", Device::getIP),[m
[32m+[m				[32mnew StringFilter<>("Owner", Device::getOwner),[m
[32m+[m				[32mnew EnumFilter<>("State", Device::getState, Device.State.class));[m
[32m+[m		[32mpaginated.setItems(Model.devices);[m
[32m+[m	[32m}[m
[32m+[m[32m}[m
