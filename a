[32m+[m[32m    public AuthController(Stage stage) {[m[41m[m
[32m+[m[32m        this.stage = stage;[m[41m[m
[32m+[m[32m        this.toggleGroup = new ToggleGroup();[m[41m[m
[32m+[m[32m        ToggleButtonsUtil.addAlwaysOneSelectedSupport(toggleGroup);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    @Override[m[41m[m
[32m+[m[32m    public void initialize(URL location, ResourceBundle resources) {[m[41m[m
[32m+[m[32m        closeIcon.addEventHandler(MouseEvent.MOUSE_CLICKED, event -> Platform.exit());[m[41m[m
[32m+[m[32m        minimizeIcon.addEventHandler(MouseEvent.MOUSE_CLICKED,[m[41m[m
[32m+[m[32m                event -> ((Stage) rootPane.getScene().getWindow()).setIconified(true));[m[41m[m
[32m+[m[32m        alwaysOnTopIcon.addEventHandler(MouseEvent.MOUSE_CLICKED, event -> {[m[41m[m
[32m+[m[32m            boolean newVal = !stage.isAlwaysOnTop();[m[41m[m
[32m+[m[32m            alwaysOnTopIcon.pseudoClassStateChanged(PseudoClass.getPseudoClass("always-on-top"), newVal);[m[41m[m
[32m+[m[32m            stage.setAlwaysOnTop(newVal);[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        windowHeader.setOnMousePressed(event -> {[m[41m[m
[32m+[m[32m            xOffset = stage.getX() - event.getScreenX();[m[41m[m
[32m+[m[32m            yOffset = stage.getY() - event.getScreenY();[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[32m        windowHeader.setOnMouseDragged(event -> {[m[41m[m
[32m+[m[32m            stage.setOpacity(0.7);[m[41m[m
[32m+[m[32m            stage.setX(event.getScreenX() + xOffset);[m[41m[m
[32m+[m[32m            stage.setY(event.getScreenY() + yOffset);[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[32m        windowHeader.setOnMouseReleased(event -> {[m[41m[m
[32m+[m[32m            stage.setOpacity(1.0);[m[41m[m
[32m+[m[32m        });[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        initializeLoader();[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    private void initializeLoader() {[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    private ToggleButton createToggle(String icon, String text) {[m[41m[m
[32m+[m[32m        return createToggle(icon, text, 0);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    private ToggleButton createToggle(String icon, String text, double rotate) {[m[41m[m
[32m+[m[32m        MFXIconWrapper wrapper = new MFXIconWrapper(icon, 24, 32);[m[41m[m
[32m+[m[32m        MFXRectangleToggleNode toggleNode = new MFXRectangleToggleNode(text, wrapper);[m[41m[m
[32m+[m[32m        toggleNode.setAlignment(Pos.CENTER_LEFT);[m[41m[m
[32m+[m[32m        toggleNode.setMaxWidth(Double.MAX_VALUE);[m[41m[m
[32m+[m[32m        toggleNode.setToggleGroup(toggleGroup);[m[41m[m
[32m+[m[32m        if (rotate != 0)[m[41m[m
[32m+[m[32m            wrapper.getIcon().setRotate(rotate);[m[41m[m
[32m+[m[32m        return toggleNode;[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
