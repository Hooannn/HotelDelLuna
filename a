[32m+[m[32mimport io.github.palexdev.materialfx.utils.RandomUtils;[m
[32m+[m[32mimport javafx.beans.property.*;[m
[32m+[m
[32m+[m[32mpublic class Device {[m
[32m+[m[32m    public enum State {[m
[32m+[m[32m        ONLINE, OFFLINE[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private final IntegerProperty id = new SimpleIntegerProperty();[m
[32m+[m[32m    private final StringProperty name = new SimpleStringProperty("");[m
[32m+[m[32m    private final StringProperty ip = new SimpleStringProperty("");[m
[32m+[m[32m    private final StringProperty owner = new SimpleStringProperty("");[m
[32m+[m[32m    private final ObjectProperty<State> state = new SimpleObjectProperty<>();[m
[32m+[m
[32m+[m[32m    public Device(int id, String name, String ip, String owner, State state) {[m
[32m+[m[32m        setID(id);[m
[32m+[m[32m        setName(name);[m
[32m+[m[32m        setIP(ip);[m
[32m+[m[32m        setOwner(owner);[m
[32m+[m[32m        setState(state);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public int getID() {[m
[32m+[m[32m        return id.get();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public IntegerProperty idProperty() {[m
[32m+[m[32m        return id;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void setID(int id) {[m
[32m+[m[32m        this.id.set(id);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public String getName() {[m
[32m+[m[32m        return name.get();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public StringProperty nameProperty() {[m
[32m+[m[32m        return name;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void setName(String name) {[m
[32m+[m[32m        this.name.set(name);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public String getIP() {[m
[32m+[m[32m        return ip.get();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public StringProperty ipProperty() {[m
[32m+[m[32m        return ip;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void setIP(String ip) {[m
[32m+[m[32m        this.ip.set(ip);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public String getOwner() {[m
[32m+[m[32m        return owner.get();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public StringProperty ownerProperty() {[m
[32m+[m[32m        return owner;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void setOwner(String owner) {[m
[32m+[m[32m        this.owner.set(owner);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public State getState() {[m
[32m+[m[32m        return state.get();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public ObjectProperty<State> stateProperty() {[m
[32m+[m[32m        return state;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void setState(State state) {[m
[32m+[m[32m        this.state.set(state);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static int randomID() {[m
[32m+[m[32m        return RandomUtils.random.nextInt(100000, 1000000);[m
[32m+[m[32m    }[m
