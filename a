[32m+[m[32m    @Override[m
[32m+[m[32m    public void initialize(URL location, ResourceBundle resources) {[m
[32m+[m[32m        FloorsService floorsService = new FloorsService();[m
[32m+[m[32m        RoomTypesService roomTypesService = new RoomTypesService();[m
[32m+[m[32m        for (Floor floor : floorsService.getFloors()) {[m
[32m+[m[32m            this.floor.getItems().add(floor);[m
[32m+[m[32m        }[m
[32m+[m[32m        for (RoomType roomType : roomTypesService.getRoomTypes()) {[m
[32m+[m[32m            this.type.getItems().add(roomType);[m
[32m+[m[32m        }[m
[32m+[m[32m        for (RoomStatus status : RoomStatus.values()) {[m
[32m+[m[32m            this.state.getItems().add(String.valueOf(status));[m
[32m+[m[32m        }[m
[32m+[m[32m        displayRoomInformation(room);[m
[32m+[m[32m        price.setDisable(true);[m
[32m+[m[32m        state.setDisable(true);[m
[32m+[m[32m        ok.setOnAction(e -> {[m
[32m+[m[32m            try {[m
[32m+[m[32m                updateRoom(e);[m
[32m+[m[32m            } catch (Exception exception) {[m
[32m+[m[32m                exception.printStackTrace();[m
[32m+[m[32m            }[m
