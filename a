[32m+[m[32mpackage com.ht.hoteldelluna.models;[m
[32m+[m
[32m+[m[32mimport com.mongodb.ConnectionString;[m
[32m+[m[32mimport com.mongodb.MongoClientSettings;[m
[32m+[m[32mimport com.mongodb.MongoException;[m
[32m+[m[32mimport com.mongodb.ServerApi;[m
[32m+[m[32mimport com.mongodb.ServerApiVersion;[m
[32m+[m[32mimport com.mongodb.client.MongoClient;[m
[32m+[m[32mimport com.mongodb.client.MongoClients;[m
[32m+[m[32mimport com.mongodb.client.MongoCollection;[m
[32m+[m[32mimport com.mongodb.client.MongoDatabase;[m
[32m+[m[32mimport com.mongodb.client.result.InsertOneResult;[m
[32m+[m
[32m+[m[32mimport java.util.ArrayList;[m
[32m+[m[32mimport java.util.List;[m
[32m+[m[32mimport java.util.stream.Collectors;[m
[32m+[m
[32m+[m[32mimport org.bson.Document;[m
[32m+[m
[32m+[m[32mpublic class DataManager {[m
[32m+[m[32m    public static DataManager shared = new DataManager();[m
[32m+[m[32m    private MongoClient client;[m
[32m+[m[32m    private MongoDatabase database;[m
[32m+[m[32m    private String connectionString = "mongodb+srv://hoanthui123:hoandaica123@default.evhwqpv.mongodb.net/?retryWrites=true&w=majority";[m
[32m+[m
[32m+[m[32m    ServerApi serverApi = ServerApi.builder()[m
[32m+[m[32m            .version(ServerApiVersion.V1)[m
[32m+[m[32m            .build();[m
[32m+[m[32m    MongoClientSettings settings = MongoClientSettings.builder()[m
[32m+[m[32m            .applyConnectionString(new ConnectionString(connectionString))[m
[32m+[m[32m            .serverApi(serverApi)[m
[32m+[m[32m            .build();[m
[32m+[m
[32m+[m[32m    public DataManager() {[m
[32m+[m[32m        try {[m
[32m+[m[32m            this.client = MongoClients.create(settings);[m
[32m+[m[32m            this.database = this.client.getDatabase("Default");[m
[32m+[m[32m            database.runCommand(new Document("ping", 1));[m
[32m+[m[32m            System.out.println("Pinged your deployment. You successfully connected to MongoDB!");[m
[32m+[m
[32m+[m[32m            initializeCollections();[m
[32m+[m[32m        } catch (MongoException e) {[m
[32m+[m[32m            e.printStackTrace();[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public MongoClient getClient() {[m
[32m+[m[32m        return this.client;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public MongoDatabase getDatabase() {[m
[32m+[m[32m        return this.database;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private void initializeCollections() {[m
[32m+[m[32m        try {[m
[32m+[m[32m            MongoCollection<Document> textCollection = this.database.getCollection("text");[m
[32m+[m[32m            if (textCollection == null) {[m
[32m+[m[32m                this.database.createCollection("text");[m
[32m+[m[32m            }[m
[32m+[m[32m        } catch (MongoException e) {[m
[32m+[m[32m            e.printStackTrace();[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public void addSampleDocument(String text) {[m
[32m+[m[32m        MongoCollection<Document> textCollection = this.database.getCollection("text");[m
[32m+[m[32m        Document doc1 = new Document("value", text);[m
[32m+[m[32m        InsertOneResult result = textCollection.insertOne(doc1);[m
[32m+[m[32m        System.out.println("Inserted a document with the following id: "[m
[32m+[m[32m                + result.getInsertedId().asObjectId().getValue());[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public List<Text> fetchSampleDocuments() {[m
[32m+[m[32m        MongoCollection<Document> textCollection = this.database.getCollection("text");[m
[32m+[m[32m        List<Text> results = new ArrayList<Text>();[m
[32m+[m[32m        textCollection.find().forEach(document -> {[m
[32m+[m[32m            Text text = new Text(document.getObjectId("_id").toString(), document.getString("value"));[m
[32m+[m[32m            results.add(text);[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        return results;[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
