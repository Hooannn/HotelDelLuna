[32m+[m[32m    private void cancelReservation() {[m
[32m+[m[32m        if (reservation == null) return;[m
[32m+[m[32m        reservationsService.closeReservation(reservation.getId().toString(), reservation.getRoom().getId().toString());[m
[32m+[m[32m        delegate.onCancelled(room);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private void cleanRoom() {[m
[32m+[m[32m        if (reservation == null) return;[m
[32m+[m[32m        reservationsService.closeReservation(reservation.getId().toString(), reservation.getRoom().getId().toString());[m
[32m+[m[32m        delegate.onCleaned(room);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private void showCheckInDialog() {[m
[32m+[m[32m        MFXButton updateButton = new MFXButton("C·∫≠p nh·∫≠t");[m
[32m+[m[32m        MFXButton exitButton = new MFXButton("Tho√°t");[m
[32m+[m[32m        FXMLLoader loader = new FXMLLoader(MFXResourcesLoader.loadURL("fxml/main/RoomManager/CheckInForm.fxml"));[m
[32m+[m[32m        loader.setControllerFactory(c -> new CheckInFormController(room, reservation));[m
[32m+[m[32m        dialogContent.setHeaderText("Check-in");[m
[32m+[m[32m        dialogContent.clearActions();[m
[32m+[m[32m        dialogContent.addActions([m
[32m+[m[32m                Map.entry(updateButton, event -> {[m
[32m+[m[32m                    CheckInFormController checkInFormController = loader.getController();[m
[32m+[m[32m                    Reservation reservation = checkInFormController.getReservation();[m
[32m+[m[32m                    InsertOneResult result = reservationsService.addReservation(reservation, room.getId().toString());[m
[32m+[m[32m                    dialog.close();[m
[32m+[m[32m                    delegate.onCheckedIn(room);[m
[32m+[m[32m                }),[m
[32m+[m[32m                Map.entry(exitButton, event -> dialog.close())[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        try {[m
[32m+[m[32m            dialogContent.setContent(loader.load());[m
[32m+[m[32m        } catch (IOException e) {[m
[32m+[m[32m            e.printStackTrace();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        dialog.showDialog();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private void showCheckOutDialog() {[m
