[32m+[m[32mimport javafx.application.Platform;[m
[32m+[m[32mimport javafx.css.PseudoClass;[m
[32m+[m[32mimport javafx.fxml.FXML;[m
[32m+[m[32mimport javafx.fxml.Initializable;[m
[32m+[m[32mimport javafx.geometry.Pos;[m
[32m+[m[32mimport javafx.scene.control.ToggleButton;[m
[32m+[m[32mimport javafx.scene.control.ToggleGroup;[m
[32m+[m[32mimport javafx.scene.image.Image;[m
[32m+[m[32mimport javafx.scene.image.ImageView;[m
[32m+[m[32mimport javafx.scene.input.MouseEvent;[m
[32m+[m[32mimport javafx.scene.layout.AnchorPane;[m
[32m+[m[32mimport javafx.scene.layout.HBox;[m
[32m+[m[32mimport javafx.scene.layout.StackPane;[m
[32m+[m[32mimport javafx.scene.layout.VBox;[m
[32m+[m[32mimport javafx.scene.shape.Circle;[m
[32m+[m[32mimport javafx.stage.Stage;[m
[32m+[m
[32m+[m[32mimport java.net.URL;[m
[32m+[m[32mimport java.util.List;[m
[32m+[m[32mimport java.util.ResourceBundle;[m
[32m+[m
[32m+[m[32mimport static com.ht.hoteldelluna.MFXResourcesLoader.loadURL;[m
[32m+[m
[32m+[m[32mpublic class MainController implements Initializable {[m
[32m+[m[32m    private final Stage stage;[m
[32m+[m[32m    private double xOffset;[m
[32m+[m[32m    private double yOffset;[m
[32m+[m[32m    private final ToggleGroup toggleGroup;[m
[32m+[m
[32m+[m[32m    @FXML[m
[32m+[m[32m    private HBox windowHeader;[m
[32m+[m
[32m+[m[32m    @FXML[m
[32m+[m[32m    private MFXFontIcon closeIcon;[m
[32m+[m
[32m+[m[32m    @FXML[m
[32m+[m[32m    private MFXFontIcon minimizeIcon;[m
[32m+[m
[32m+[m[32m    @FXML[m
[32m+[m[32m    private MFXFontIcon alwaysOnTopIcon;[m
[32m+[m
[32m+[m[32m    @FXML[m
[32m+[m[32m    private AnchorPane rootPane;[m
[32m+[m
[32m+[m[32m    @FXML[m
[32m+[m[32m    private MFXScrollPane scrollPane;[m
[32m+[m
[32m+[m[32m    @FXML[m
[32m+[m[32m    private VBox navBar;[m
[32m+[m
[32m+[m[32m    @FXML[m
[32m+[m[32m    private StackPane contentPane;[m
[32m+[m
[32m+[m[32m    @FXML[m
[32m+[m[32m    private StackPane logoContainer;[m
[32m+[m
[32m+[m[32m    public MainController(Stage stage) {[m
[32m+[m[32m        this.stage = stage;[m
[32m+[m[32m        this.toggleGroup = new ToggleGroup();[m
[32m+[m[32m        ToggleButtonsUtil.addAlwaysOneSelectedSupport(toggleGroup);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void initialize(URL location, ResourceBundle resources) {[m
[32m+[m[32m        closeIcon.addEventHandler(MouseEvent.MOUSE_CLICKED, event -> Platform.exit());[m
[32m+[m[32m        minimizeIcon.addEventHandler(MouseEvent.MOUSE_CLICKED,[m
[32m+[m[32m                event -> ((Stage) rootPane.getScene().getWindow()).setIconified(true));[m
[32m+[m[32m        alwaysOnTopIcon.addEventHandler(MouseEvent.MOUSE_CLICKED, event -> {[m
[32m+[m[32m            boolean newVal = !stage.isAlwaysOnTop();[m
[32m+[m[32m            alwaysOnTopIcon.pseudoClassStateChanged(PseudoClass.getPseudoClass("always-on-top"), newVal);[m
[32m+[m[32m            stage.setAlwaysOnTop(newVal);[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        windowHeader.setOnMousePressed(event -> {[m
[32m+[m[32m            xOffset = stage.getX() - event.getScreenX();[m
[32m+[m[32m            yOffset = stage.getY() - event.getScreenY();[m
[32m+[m[32m        });[m
[32m+[m[32m        windowHeader.setOnMouseDragged(event -> {[m
[32m+[m[32m            stage.setX(event.getScreenX() + xOffset);[m
[32m+[m[32m            stage.setY(event.getScreenY() + yOffset);[m
[32m+[m[32m        });[m
[32m+[m
[32m+[m[32m        initializeLoader();[m
[32m+[m
[32m+[m[32m        ScrollUtils.addSmoothScrolling(scrollPane);[m
[32m+[m
[32m+[m[32m        // The only way to get a fucking smooth image in this shitty framework[m
[32m+[m[32m        Image image = new Image(MFXResourcesLoader.load("logo_alt.png"), 64, 64, true, true);[m
[32m+[m[32m        ImageView logo = new ImageView(image);[m
[32m+[m[32m        Circle clip = new Circle(30);[m
[32m+[m[32m        logo.setClip(clip);[m
[32m+[m[32m        logoContainer.getChildren().add(logo);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private void initializeLoader() {[m
[32m+[m[32m        MFXLoader loader = new MFXLoader();[m
[32m+[m[32m        loader.addView(MFXLoaderBean.of("MONGOTESTING", loadURL("fxml/MongoTesting.fxml"))[m
[32m+[m[32m                .setBeanToNodeMapper(() -> createToggle("fas-database", "Mongo testing")).setDefaultRoot(true).get());[m
[32m+[m
[32m+[m[32m        loader.addView(MFXLoaderBean.of("BUTTONS", loadURL("fxml/Buttons.fxml"))[m
[32m+[m[32m                .setBeanToNodeMapper(() -> createToggle("fas-circle-dot", "Buttons")).get());[m
[32m+[m
[32m+[m[32m        loader.setOnLoadedAction(beans -> {[m
[32m+[m[32m            List<ToggleButton> nodes = beans.stream()[m
[32m+[m[32m                    .map(bean -> {[m
[32m+[m[32m                        ToggleButton toggle = (ToggleButton) bean.getBeanToNodeMapper().get();[m
[32m+[m[32m                        toggle.setOnAction(event -> contentPane.getChildren().setAll(bean.getRoot()));[m
[32m+[m[32m                        if (bean.isDefaultView()) {[m
[32m+[m[32m                            contentPane.getChildren().setAll(bean.getRoot());[m
[32m+[m[32m                            toggle.setSelected(true);[m
[32m+[m[32m                        }[m
[32m+[m[32m                        return toggle;[m
[32m+[m[32m                    })[m
[32m+[m[32m                    .toList();[m
[32m+[m[32m            navBar.getChildren().setAll(nodes);[m
[32m+[m[32m        });[m
[32m+[m[32m        loader.start();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private ToggleButton createToggle(String icon, String text) {[m
[32m+[m[32m        return createToggle(icon, text, 0);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private ToggleButton createToggle(String icon, String text, double rotate) {[m
[32m+[m[32m        MFXIconWrapper wrapper = new MFXIconWrapper(icon, 24, 32);[m
[32m+[m[32m        MFXRectangleToggleNode toggleNode = new MFXRectangleToggleNode(text, wrapper);[m
[32m+[m[32m        toggleNode.setAlignment(Pos.CENTER_LEFT);[m
[32m+[m[32m        toggleNode.setMaxWidth(Double.MAX_VALUE);[m
[32m+[m[32m        toggleNode.setToggleGroup(toggleGroup);[m
[32m+[m[32m        if (rotate != 0)[m
[32m+[m[32m            wrapper.getIcon().setRotate(rotate);[m
[32m+[m[32m        return toggleNode;[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
