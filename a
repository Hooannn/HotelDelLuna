[32m+[m[32mext {[m
[32m+[m[32m    arcgisVersion = '200.2.0'[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mrepositories {[m
[32m+[m[32m    mavenCentral()[m
[32m+[m[32m    maven {[m
[32m+[m[32m        url 'https://esri.jfrog.io/artifactory/arcgis'[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mconfigurations {[m
[32m+[m[32m    natives[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mdependencies {[m
[32m+[m[32m    implementation "com.esri.arcgisruntime:arcgis-java:$arcgisVersion"[m
[32m+[m[32m    natives "com.esri.arcgisruntime:arcgis-java-jnilibs:$arcgisVersion"[m
[32m+[m[32m    natives "com.esri.arcgisruntime:arcgis-java-resources:$arcgisVersion"[m
[32m+[m[32m    // handle SLF4J http://www.slf4j.org/codes.html#StaticLoggerBinder using a no-operation logger[m
[32m+[m[32m    implementation "org.slf4j:slf4j-nop:2.0.5"[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mjavafx {[m
[32m+[m[32m    version = "17.0.8"[m
[32m+[m[32m    modules = [ 'javafx.controls', 'javafx.graphics', 'javafx.fxml', 'javafx.web', 'javafx.media' ][m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mapplication {[m
[32m+[m[32m    mainModule = "com.mycompany.app"[m
[32m+[m[32m    mainClass = "com.mycompany.app.App"[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mtask copyNatives(type: Copy) {[m
[32m+[m[32m    description = "Copies the arcgis native libraries into the .arcgis directory for development."[m
[32m+[m[32m    group = "build"[m
[32m+[m[32m    configurations.natives.asFileTree.each {[m
[32m+[m[32m        from(zipTree(it))[m
[32m+[m[32m    }[m
[32m+[m[32m    into "${System.properties.getProperty("user.home")}/.arcgis/$arcgisVersion"[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mrun {[m
[32m+[m[32m    dependsOn copyNatives[m
[32m+[m[32m}[m
