[32m+[m[32m * WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied. See the[m[41m [m
[32m+[m[32m * License for the specific language governing permissions and limitations[m[41m [m
[32m+[m[32m * under the License.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mplugins {[m
[32m+[m[32m    id 'application'[m
[32m+[m[32m    id 'idea'[m
[32m+[m[32m    id 'org.openjfx.javafxplugin' version '0.1.0'[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32midea {[m
[32m+[m[32m    module {[m
[32m+[m[32m        downloadJavadoc = true[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
