[32m+[m[32mpackage com.ht.hoteldelluna.backend;[m
[32m+[m
[32m+[m[32mimport com.google.gson.Gson;[m
[32m+[m[32mimport com.google.gson.JsonSyntaxException;[m
[32m+[m[32mimport com.ht.hoteldelluna.models.Floor;[m
[32m+[m[32mimport org.bson.Document;[m
[32m+[m
[32m+[m[32mimport java.util.List;[m
[32m+[m
[32m+[m[32mpublic class Parser {[m
[32m+[m[32m    private final Gson gson = new Gson();[m
[32m+[m[32m    public Parser() {}[m
[32m+[m
[32m+[m[32m    public <T> List<T> fromDocuments(List<Document> documents, Class<T> classOfT) {[m
[32m+[m[32m        return documents.stream().map(document -> gson.fromJson(document.toJson(), classOfT)).toList();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public <T> T fromDocument(Document document, Class<T> classOfT) {[m
[32m+[m[32m        return gson.fromJson(document.toJson(), classOfT);[m
[32m+[m[32m    }[m
[32m+[m[32m    public <T> Document toDocument(T model) {[m
[32m+[m[32m        return Document.parse(gson.toJson(model));[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public <T> List<Document> toDocuments(List<T> models) {[m
[32m+[m[32m        return models.stream().map((model) -> Document.parse(gson.toJson(model))).toList();[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
