[32m+[m[32mimport java.net.URL;[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Utility class which manages the access to this project's assets.[m
[32m+[m[32m * Helps keeping the assets files structure organized.[m
[32m+[m[32m */[m
[32m+[m[32mpublic class MFXResourcesLoader {[m
[32m+[m
[32m+[m[32m    private MFXResourcesLoader() {[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static URL loadURL(String path) {[m
[32m+[m[32m        return MFXResourcesLoader.class.getResource(path);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static String load(String path) {[m
[32m+[m[32m        return loadURL(path).toString();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static InputStream loadStream(String name) {[m
[32m+[m[32m        return MFXResourcesLoader.class.getResourceAsStream(name);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m}[m
