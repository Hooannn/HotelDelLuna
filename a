[32m+[m[32mpublic class MongoTestingController implements Initializable {[m
[32m+[m[32m    DataManager dm = DataManager.shared;[m
[32m+[m[32m    ObservableList<Text> sampleDocuments;[m
[32m+[m
[32m+[m[32m    @FXML[m
[32m+[m[32m    private MFXTextField textField;[m
[32m+[m
[32m+[m[32m    @FXML[m
[32m+[m[32m    private MFXButton submitButton;[m
[32m+[m
[32m+[m[32m    @FXML[m
[32m+[m[32m    private Label headerLabel;[m
[32m+[m
[32m+[m[32m    @FXML[m
[32m+[m[32m    private MFXPaginatedTableView<Text> paginatedTable;[m
[32m+[m
[32m+[m[32m    @Override[m
[32m+[m[32m    public void initialize(URL url, ResourceBundle resourceBundle) {[m
[32m+[m[32m        paginatedTable.autosizeColumnsOnInitialization();[m
[32m+[m[32m        When.onChanged(paginatedTable.currentPageProperty())[m
[32m+[m[32m                .then((oldValue, newValue) -> paginatedTable.autosizeColumns())[m
[32m+[m[32m                .listen();[m
[32m+[m
[32m+[m[32m        submitButton.setOnAction(this::handleSubmitButtonClick);[m
[32m+[m
[32m+[m[32m        fetchDocuments();[m
[32m+[m
[32m+[m[32m        setupPaginated();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private void handleSubmitButtonClick(ActionEvent event) {[m
[32m+[m[32m        String textFromTextField = textField.getText();[m
[32m+[m[32m        dm.addSampleDocument(textFromTextField);[m
[32m+[m[32m        textField.clear();[m
[32m+[m
[32m+[m[32m        fetchDocuments();[m
[32m+[m[32m        paginatedTable.setItems(sampleDocuments);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private void fetchDocuments() {[m
[32m+[m[32m        this.sampleDocuments = FXCollections.observableList(dm.fetchSampleDocuments());[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private void setupPaginated() {[m
[32m+[m[32m        MFXTableColumn<Text> idColumn = new MFXTableColumn<>("ID", false, Comparator.comparing(Text::getId));[m
[32m+[m[32m        MFXTableColumn<Text> valueColumn = new MFXTableColumn<>("Value", false,[m
[32m+[m[32m                Comparator.comparing(Text::getValue));[m
[32m+[m
[32m+[m[32m        idColumn.setRowCellFactory(device -> new MFXTableRowCell<>(Text::getId));[m
[32m+[m[32m        valueColumn.setRowCellFactory(device -> new MFXTableRowCell<>(Text::getValue));[m
[32m+[m
[32m+[m[32m        paginatedTable.getTableColumns().addAll(idColumn, valueColumn);[m
[32m+[m[32m        paginatedTable.setItems(sampleDocuments);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
