[32m+[m[32mpublic class Helper {[m
[32m+[m[32m    public static String formatCurrency(double amount) {[m
[32m+[m[32m        Locale vietnamLocale = new Locale("vi", "VN");[m
[32m+[m[32m        NumberFormat vndFormat = NumberFormat.getCurrencyInstance(vietnamLocale);[m
[32m+[m[32m        vndFormat.setCurrency(Currency.getInstance("VND"));[m
[32m+[m[32m        return vndFormat.format(amount);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
